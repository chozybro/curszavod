          "values": [
            {
              "role": "system",
              "content": "You are an expert SEO researcher. Your task is to research a topic thoroughly before writing about it. Provide detailed insights, statistics, and key points that should be covered."
            },
            {
              "role": "user",
              "content": "Research the following topic in detail: {{ $json.topic }}\n\nKeywords to focus on: {{ $json.keywords }}\n\nProvide:\n1. Key statistics and facts\n2. Main subtopics to cover\n3. Expert insights\n4. Current trends\n5. Common questions people ask\n\nLanguage: {{ $json.language }}"
            }
