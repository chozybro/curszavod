    {
      "parameters": {
        "jsCode": "const images = $input.all();\nconst articleContent = $('Store Article Content').item.json.articleContent;\nconst articleTitle = $('Store Article Content').item.json.articleTitle;\n\n// Build featured image ID (first image)\nconst featuredImageId = images[0].json.id;\n\n// Insert images into article content\nlet enhancedContent = articleContent;\n\n// Add featured image at the beginning\nif (images.length > 0) {\n  enhancedContent = `<figure class=\"wp-block-image\"><img src=\"${images[0].json.source_url}\" alt=\"${images[0].json.alt_text}\" class=\"wp-image-${images[0].json.id}\"/></figure>\\n\\n` + enhancedContent;\n}\n\n// Insert other images throughout the content\nconst contentSections = enhancedContent.split('</h2>');\nfor (let i = 1; i < Math.min(images.length, contentSections.length); i++) {\n  if (contentSections[i]) {\n    contentSections[i] = `<figure class=\"wp-block-image\"><img src=\"${images[i].json.source_url}\" alt=\"${images[i].json.alt_text}\" class=\"wp-image-${images[i].json.id}\"/></figure>\\n\\n` + contentSections[i];\n  }\n}\n\nenhancedContent = contentSections.join('</h2>');\n\nreturn {\n  json: {\n    title: articleTitle,\n    content: enhancedContent,\n    featuredImageId: featuredImageId,\n    topic: $('Extract Variables').item.json.topic,\n    keywords: $('Extract Variables').item.json.keywords,\n    rowId: $('Extract Variables').item.json.rowId\n  }\n};"
      },
